{
    "name": "Google Calendar: Add AND remove calendars",
    "description": "Chrome extension to turn the \"Add a coworker's calendar\" field in Google Calendar into a toggle for adding AND removing calendars.",
    "version": "0.1",
    "content_scripts": [ {
        "matches": ["https://www.google.com/calendar/*", "http://www.google.com/calendar/*"],
        "js": ["node_modules/email-addresses/lib/email-addresses.js",
               "node_modules/underscore/underscore.js",
               "index.js"],
               "run_at": "document_end"
    } ], 
    "permissions": [
        "https://www.google.com/calendar/*"
    ],
    "icons": {
        "128": "icon128.png"
    },
    "manifest_version": 2
}
